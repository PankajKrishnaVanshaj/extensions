{
  "manifest_version": 3,
  "name": "Pk Browser Cleaner",
  "version": "1.0",
  "description": "A simple extension to clean browser data and caches",
  "permissions": ["storage", "browsingData"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
